Задание 1.
#include <stdio.h>

int main()
{
    int A, B, a, b;
    
    printf ("Введите A\n");
    scanf ("%d", &A);
    printf ("Введите B\n");
    scanf ("%d", &B);
    a = A;
    b = B;
    A = b;
    B = a;
    printf("A = %d\n", A);
    printf("B = %d\n", B);
    return 0;
}

Задание 2.
#include <stdio.h>

int main()
{
    int A, B, C, a, b, c;
    
    printf ("Введите A\n");
    scanf ("%d", &A);
    printf ("Введите B\n");
    scanf ("%d", &B);
    printf ("Введите C\n");
    scanf ("%d", &C);
    a = A;
    b = B;
    c = C;
    A = c;
    B = a;
    C = b;
    printf("A = %d\n", A);
    printf("B = %d\n", B);
    printf("C = %d\n", C);
    return 0;
}

Задание 3.
#include <stdio.h>

int main()
{
    int A, B, C, a, b, c;
    
    printf ("Введите A\n");
    scanf ("%d", &A);
    printf ("Введите B\n");
    scanf ("%d", &B);
    printf ("Введите C\n");
    scanf ("%d", &C);
    a = A;
    b = B;
    c = C;
    A = b;
    B = c;
    C = a;
    printf("A = %d\n", A);
    printf("B = %d\n", B);
    printf("C = %d\n", C);
    return 0;
}

Задание 4.
#include <stdio.h>
#include <math.h>

int
main ()
{
  int x, x1, y;

  printf ("Введите x\n");
  scanf ("%d", &x);
  x1 = pow(x, 6);
  x = pow(x, 2);
  y = 3 * x1 - 6 * x - 7; 
  printf ("y = 3x^6 − 6x^2 − 7  = %d\n", y);
  return 0;
}
