Задание 1.
#include <stdio.h>

int main ()
{
    int N;
    
    printf ("Введите количество секунд\n");
    scanf ("%d", &N);
    do {
        N = N - 60;
    } while (N > 60);
    printf ("%d - количество секунд, прошедших с начала последней минуты.\n", N);
    
    return 0;
}

Задание 2.
#include <stdio.h>

int main ()
{
    int K, a;
    
    printf ("Введите K\n");
    scanf ("%d", &K);
    if (K >= 1 && K <= 365)
    {
        if (K >=7 && K <= 365)
        {
            a = K / 7;
            K = K - a * 7;
        }
        if (K == 0)
         {
             printf ("Воскресенье.\n");
         }
        else if (K == 1)
         {
            printf ("Понедельник.\n");
         }
        else if (K == 2)
         {
            printf ("Вторник.\n");
         }
        else if (K == 3)
        {
            printf ("Среда.\n");
        }
        else if (K == 4)
        {
            printf ("Четверг.\n");
        }
        else if (K == 5)
        {
            printf ("Пятница.\n");
        }
        else if (K == 6)
        {
            printf ("Суббота.\n");
        }
    }
    else
    {
        printf ("K лежит в диапазоне 1–365\n");
    }
    
    return 0;
}

Задание 3.
#include <stdio.h>

int main ()
{
    int K, a, N;
    
    printf ("Введите K\n");
    scanf ("%d", &K);
    printf ("Введите N\n");
    scanf ("%d", &N);
    if (N >= 1 && N <= 7)
    {
        if (K >= 1 && K <= 365)
        {
            if (K >=7 && K <= 365)
            {
                a = K / 7;
                K = K - a * 7;
            }
            if (K == 1)
            {
                K = N;
            }
            else if (K == 2)
            {
                K = N + 1;
                if (K > 7)
                {
                    K = K - 7;
                }
            }
            else if (K == 3)
            {
                K = N + 2;
                if (K > 7)
                {
                    K = K - 7;
                }
            }
            else if (K == 4)
            {
                K = N + 3;
                if (K > 7)
                {
                    K = K - 7;
                }
            }
            else if (K == 5)
            {
                K = N + 4;
                if (K > 7)
                {
                    K = K - 7;
                }
            }
            else if (K == 6)
            {
                K = N + 5;
                if (K > 7)
                {
                    K = K - 7;
                }
            }
            else if (K == 0)
            {
                K = N + 6;
                if (K > 7)
                {
                    K = K - 7;
                }
            }
            if (K == 7)
            {
                printf ("Воскресенье.\n");
            }
            else if (K == 1)
            {
                printf ("Понедельник.\n");
            }
            else if (K == 2)
            {
                printf ("Вторник.\n");
            }
            else if (K == 3)
            {
                printf ("Среда.\n");
            }
            else if (K == 4)
            {
                printf ("Четверг.\n");
            }
            else if (K == 5)
            {
                printf ("Пятница.\n");
            }
            else if (K == 6)
            {
                printf ("Суббота.\n");
            }
        }
        else
        {
            printf ("K лежит в диапазоне 1–365\n");
        }
    }
    else
        {
            if (K < 1 || K > 366)
            {
                printf ("K лежит в диапазоне 1–365\n");
            }
            printf ("N лежит в диапазоне 1–7\n");
        }
       
    return 0;
}
