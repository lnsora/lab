Задание 1.
#include <stdio.h>

int main ()
{
    int A, B;
    
    printf ("Введите A:\n");
    scanf ("%d", &A);
    printf ("Введите B:\n");
    scanf ("%d", &B);
    if (A == B)
    {
        A = 0;
        B = 0;
        printf ("A = %d, B = %d.\n", A, B);
    }
    else 
    {
        if (A > B)
        {
            B = A;
            printf ("A = %d, B = %d.\n", A, B);
        }
        else 
        {
            A = B;
            printf ("A = %d, B = %d.\n", A, B);
        }
    }
    
    return 0;
}

Задание 2.
#include <stdio.h>

int main ()
{
    int A, B, C, sum;
    
    printf ("Введите A:\n");
    scanf ("%d", &A);
    printf ("Введите B:\n");
    scanf ("%d", &B);
    printf ("Введите C:\n");
    scanf ("%d", &C);
    if (C > A && B > A)
    {
        sum = C + B;
        printf ("Сумма двух наибольших из них - %d\n", sum);
    }
    else if (C > B && A > B)
    {
        sum = C + A;
        printf ("Сумма двух наибольших из них - %d\n", sum);
    }
    else if (A > C && B > C)
    {
        sum = A + B;
        printf ("Сумма двух наибольших из них - %d\n", sum);
    }
    
    return 0;
}

Задание 3.
#include <stdio.h>

int main ()
{
    int A, B, C, AC, AB, sum;
    
    printf ("Введите значения A, B, C\n");
    scanf ("%d%d%d", &A, &B, &C);
    AC = C - A;
    AC = abs (AC);
    AB = B - A;
    AB = abs (AB);
    if (AB < AC)
    {
        printf ("Ближе к точке А находится точка B.\n");
        printf ("Расстояние - %d\n", AB);
    }
    else 
    {
        printf ("Ближе к точке А находится точка C.\n");
        printf ("Расстояние - %d\n", AC);
    }
    
    return 0;
}

Задание 4.
#include <stdio.h>

int main ()
{
    int x, y;
    
    printf ("Введите координату:\n");
    scanf ("%d%d", &x, &y);
    if (x > 0 && y > 0)
    {
        printf ("Первая четверть.\n");
    }
    else if (x < 0 && y > 0)
    {
        printf ("Вторая четверть.\n");
    }
    else if (x < 0 && y < 0)
    {
        printf ("Третья четверть.\n");
    }
    else if (x > 0 && y < 0)
    {
        printf ("Четвертая четверть.\n");
    }
    
    return 0;
}

Задание 5.
#include <stdio.h>

int main ()
{
    int x, y;
    
    printf ("Введите число:\n");
    scanf ("%d", &x);
    if (x > 0)
    {
        y = x % 2;
        if (y == 0)
        {
            printf ("Положительное четное число.\n");
        }
        else
        {
            printf ("Положительное нечетное число.\n");
        }
    }
    else if (x < 0)
    {
        y = x % 2;
        if (y == 0)
        {
            printf ("Отрицательное четное число.\n");
        }
        else
        {
            printf ("Отрицательное нечетное число.\n");
        }
    }
    else if (x == 0)
    {
        printf ("Нулевое число.\n");
    }
    
    return 0;
}

Задание 6.
#include <stdio.h>

int main ()
{
    int x, y;
    
    printf ("Введите число:\n");
    scanf ("%d", &x);
    if (x > 0 && x < 1000)
    {
        if (x < 100 && x > 9)
        {
            y = x % 2;
            if (y == 0)
            {
                printf ("Двузначное четное число.\n");
            }
            else
            {
                printf ("Двузначное нечетное число.\n");
            }
        }
        else if (x < 10)
        {
            y = x % 2;
            if (y == 0)
            {
                printf ("Однозначное четное число.\n");
            }
            else
            {
                printf ("Однозначное нечетное число.\n");
            }
        }
        else if (x > 99)
        {
            y = x % 2;
            if (y == 0)
            {
                printf ("Трехзначное четное число.\n");
            }
            else
            {
                printf ("Трехзначное нечетное число.\n");
            }
        }
    }
    else
    {
        printf ("Число не лежит в диапозоне.\n");
    }
    
    return 0;
}
